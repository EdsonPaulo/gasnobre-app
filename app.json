      "useNextNotificationsApi": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okulikapaco.nobiva",
      "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
